{
  "vhost": "kade",

  "telemetry": {
      "exchange": "telemetry",
      "queues":
      [
	  {
	      "name": "hammerspace.to-aws",
	      "routing_key": "hammerspace.#"
	  },
	  {
	      "name": "catalog.to-aws",
	      "routing_key": "catalog.#"
	  }
      ]
  },

  "events": {
      "exchange": "events",
      "queues":
      [
	  {
	      "name": "events.to-aws",
	      "routing_key": "#"
	  }
      ]
  },

  "performance": {
      "exchange": "performance",
      "queues":
      [
	  {
	      "performance.to-aws",
	      "routing_key": "#"
	  }
      ]
  },

  "commands": {
      "exchange": "commands",
      "queues":
      [
	  {
	      "hammerspace.from-aws",
	      "routing_key": "hammerspace.#"
	  },
	  {
	      "catalog.from-aws",
	      "routing_key": "catalog.#"
	  }
      ]
  }
}
